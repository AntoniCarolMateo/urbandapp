<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.MusicoActivity">



    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="33dp"
        android:layout_height="47dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="@string/musicalnote"
        app:layout_constraintBottom_toBottomOf="@+id/ratingBar_exp_musico"
        app:layout_constraintStart_toEndOf="@+id/ratingBar_exp_musico"
        app:srcCompat="@drawable/ic_action_music_2" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="DescripciÃ³n"
        app:layout_constraintEnd_toEndOf="@+id/textView_descr_musico"
        app:layout_constraintStart_toStartOf="@+id/textView_descr_musico"
        app:layout_constraintTop_toTopOf="@+id/guideline17" />

    <View
        android:id="@+id/view3"
        android:layout_width="365dp"
        android:layout_height="110dp"
        android:layout_marginTop="44dp"
        android:background="@color/primaryLightColor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView_photo_musico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:contentDescription="@string/profile_picture"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/textView_username_musico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:textColor="@color/primaryTextColor"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="@+id/view3"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/view3"
        app:layout_constraintTop_toBottomOf="@+id/imageView_photo_musico" />

    <TextView
        android:id="@+id/text_exp"
        android:layout_width="105dp"
        android:layout_height="31dp"
        android:fontFamily="@font/nova_square"
        android:text="@string/experi_ncia"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/ratingBar_exp_musico"
        app:layout_constraintEnd_toEndOf="@+id/ratingBar_exp_musico"
        app:layout_constraintStart_toStartOf="@+id/ratingBar_exp_musico"
        app:layout_constraintTop_toBottomOf="@+id/group2" />

    <RatingBar
        android:id="@+id/ratingBar_exp_musico"
        android:layout_width="237dp"
        android:layout_height="41dp"
        android:layout_marginBottom="20dp"
        android:isIndicator="true"
        app:layout_constraintBottom_toTopOf="@+id/guideline16"
        app:layout_constraintEnd_toStartOf="@+id/guideline22"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="@+id/guideline21" />

    <TextView
        android:id="@+id/textView_descr_musico"
        android:layout_width="141dp"
        android:layout_height="75dp"
        android:layout_marginTop="36dp"
        android:text="@string/desc_from_api"
        android:textAlignment="center"
        app:layout_constraintStart_toStartOf="@+id/guideline21"
        app:layout_constraintTop_toTopOf="@+id/guideline17" />

    <TextView
        android:id="@+id/textView_rol_MUSICO"
        android:layout_width="66dp"
        android:layout_height="17dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/nova_square"
        android:text="@string/rol"
        app:layout_constraintStart_toStartOf="@+id/imageView_rol_public"
        app:layout_constraintTop_toBottomOf="@+id/imageView_rol_public" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="188dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="310dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="430dp" />

    <androidx.cardview.widget.CardView
        android:id="@+id/suscr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/textView_descr_musico"
        app:layout_constraintEnd_toStartOf="@+id/guideline22"
        app:layout_constraintStart_toEndOf="@+id/textView_descr_musico"
        app:layout_constraintTop_toBottomOf="@+id/textView17">

        <ImageView
            android:id="@+id/imageView_susc"
            android:layout_width="102dp"
            android:layout_height="83dp"
            app:srcCompat="@drawable/unsuscr" />

        <TextView
            android:id="@+id/textView_susc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|center_horizontal"
            android:fontFamily="@font/nova_square"
            android:gravity="bottom|center|center_vertical"
            android:text="TextView"
            android:textAllCaps="true"
            android:textColor="@color/primaryDarkColor" />
    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:contentDescription="@string/profile_picture"
        app:constraint_referenced_ids="view3,imageView_photo_musico, textView_username_musico"
        app:layout_constraintBottom_toTopOf="@+id/guideline15"
        app:layout_constraintEnd_toStartOf="@+id/guideline22"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/guideline21"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="711dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.04136253" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintGuide_percent="0.96" />

    <ImageView
        android:id="@+id/imageView_rol_public"
        android:layout_width="71dp"
        android:layout_height="63dp"
        android:layout_marginStart="92dp"
        android:contentDescription="@string/rol_usuario_perfil"
        app:layout_constraintStart_toStartOf="@+id/guideline21"
        app:layout_constraintTop_toTopOf="@+id/guideline16"
        app:srcCompat="@drawable/musician" />

    <ImageView
        android:id="@+id/imageView_gender_public"
        android:layout_width="69dp"
        android:layout_height="65dp"
        android:layout_marginEnd="84dp"
        android:contentDescription="@string/icono_sexo_per_fil"
        app:layout_constraintEnd_toStartOf="@+id/guideline22"
        app:layout_constraintTop_toTopOf="@+id/guideline16"
        app:srcCompat="@drawable/ic_media_pause_dark" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="33dp"
        android:layout_height="47dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="@string/musicalnote"
        app:layout_constraintBottom_toBottomOf="@+id/ratingBar_exp_musico"
        app:layout_constraintEnd_toStartOf="@+id/ratingBar_exp_musico"
        app:srcCompat="@drawable/ic_action_music_2" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="39dp"
        android:layout_height="44dp"
        android:layout_marginEnd="28dp"
        android:contentDescription="@string/musicalnote"
        app:layout_constraintEnd_toStartOf="@+id/guideline22"
        app:layout_constraintTop_toTopOf="@+id/guideline16"
        app:srcCompat="@drawable/ic_action_music_1" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="33dp"
        android:layout_height="46dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="40dp"
        android:contentDescription="@string/musicalnote"
        app:layout_constraintStart_toStartOf="@+id/guideline21"
        app:layout_constraintTop_toTopOf="@+id/guideline16"
        app:srcCompat="@drawable/ic_action_music_1" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group_notes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="imageView11,imageView7" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group_etiquetes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:constraint_referenced_ids="text_exp,textView17" />

</androidx.constraintlayout.widget.ConstraintLayout>